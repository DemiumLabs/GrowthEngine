#!/bin/bash

####################
###   Binaries   ###
####################
DOCKER=$(which docker)
####################

PROJECT_PATH="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && cd .. && pwd )"

while read line; do export "$line";
done < <(cat ${PROJECT_PATH}/.env | grep -v "#" | grep -v "^$")

${DOCKER} stack deploy -c ${PROJECT_PATH}/swarm-compose.yml noraina-api;